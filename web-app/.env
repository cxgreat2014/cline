# Server Configuration
NODE_ENV=development
PORT=8000
CLIENT_URL=http://localhost:3000

# Database
DATABASE_PATH=./data/cline.db

# Authentication
JWT_SECRET=cline-web-development-secret-key-2024
JWT_EXPIRES_IN=7d

# API Keys
ANTHROPIC_API_KEY=your-anthropic-api-key
OPENAI_API_KEY=your-openai-api-key
GOOGLE_API_KEY=your-google-api-key

# File Upload
MAX_FILE_SIZE=10485760  # 10MB
UPLOAD_DIR=./uploads

# Security
BCRYPT_ROUNDS=12
RATE_LIMIT_WINDOW_MS=900000  # 15 minutes
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
LOG_FILE=./logs/cline.log

# Repository Settings
MAX_REPOSITORIES_PER_USER=10
DEFAULT_REPOSITORY_PERMISSIONS={"read":true,"write":true,"execute":false,"admin":false}

# Task Settings
MAX_CONCURRENT_TASKS=3
TASK_TIMEOUT_MS=3600000  # 1 hour
AUTO_SAVE_INTERVAL_MS=30000  # 30 seconds

# Terminal Settings
TERMINAL_TIMEOUT_MS=300000  # 5 minutes
MAX_TERMINAL_SESSIONS=5

# MCP Settings
MCP_TIMEOUT_MS=30000  # 30 seconds
MCP_MAX_SERVERS=10

# Development Settings (only for development)
ENABLE_CORS=true
ENABLE_MORGAN_LOGGING=true
ENABLE_DETAILED_ERRORS=true
